<?xml version="1.0" encoding="utf-8" ?>
<application xmlns="http://ns.adobe.com/air/application/25.0">
    <id>net.calii.test.VideoTest</id>
    <filename>RotationTest</filename>
    <name>VideoTest</name>
    <versionNumber>0.0.12</versionNumber>
    <initialWindow>
        <content>SWF file name is set automatically at compile time</content>
        <visible>true</visible>
        <aspectRatio>landscape</aspectRatio>
        <autoOrients>true</autoOrients>
        <fullScreen>false</fullScreen>
        <renderMode>direct</renderMode>
        <depthAndStencil>true</depthAndStencil>
        <softKeyboardBehavior>none</softKeyboardBehavior>
    </initialWindow>
    <icon>
        <image29x29>icons/Icon-Small.png</image29x29>
        <image40x40>icons/Icon-Small-40.png</image40x40>
        <image50x50>icons/Icon-Small-50.png</image50x50>
        <image57x57>icons/Icon.png</image57x57>
        <image58x58>icons/Icon-Small@2x.png</image58x58>
        <image60x60>icons/Icon-60.png</image60x60>
        <image72x72>icons/Icon-72.png</image72x72>
        <image76x76>icons/Icon-76.png</image76x76>
        <image80x80>icons/Icon-Small-40@2x.png</image80x80>
        <image100x100>icons/Icon-Small-50@2x.png</image100x100>
        <image114x114>icons/Icon@2x.png</image114x114>
        <image120x120>icons/Icon-60@2x.png</image120x120>
        <image144x144>icons/Icon-72@2x.png</image144x144>
        <image152x152>icons/Icon-76@2x.png</image152x152>
        <image512x512>icons/iTunesArtwork.png</image512x512>
        <image1024x1024>icons/iTunesArtwork@2x.png</image1024x1024>
    </icon>
    <iPhone>
        <InfoAdditions>
            <![CDATA[
            <key>UIDeviceFamily</key>
            <array>
                <!-- iPhone and iPod touch devices -->
                <string>1</string>
                <!-- iPad devices -->
                <string>2</string>
            </array>
            <key>ITSAppUsesNonExemptEncryption</key><false/>
            ]]>
        </InfoAdditions>

        <Entitlements>
            <![CDATA[
            <key>get-task-allow</key>
                    <false/>
            <key>beta-reports-active</key>
                <true/>
            ]]>
        </Entitlements>
        <requestedDisplayResolution>high</requestedDisplayResolution>
    </iPhone>
</application>
